{
  "name": "movies-app",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "watch:src": "watch \"npm run build:prod -s\" src",
    "start": "webpack-dev-server --config webpack.dev.js",
    "build:dev": "npm run clean && npm run lint && cross-env NODE_ENV=development webpack --config webpack.dev.js",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "clean": "node ./scripts/clean.js",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest",
    "test:watch": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --watch",
    "test:coverage": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --coverage"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/plugin-transform-modules-commonjs": "^7.12.13",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.8",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^5.0.0",
    "react-svg-loader": "^3.0.3",
    "sass-loader": "^11.0.1",
    "watch": "^1.0.2",
    "webpack": "^5.23.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
